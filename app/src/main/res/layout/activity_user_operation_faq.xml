<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_personal_message_maintain"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:orientation="vertical"
    android:background="@color/white"
    tools:context=".ui.UserOperationFAQActivity">
    <RelativeLayout
        android:id="@+id/title"
        android:background="@color/white"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_96">
        <ImageView
            android:layout_marginLeft="@dimen/px_32"
            android:src="@mipmap/zuojian_hei"
            android:layout_centerVertical="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:layout_centerInParent="true"
            android:textColor="@color/cl_333333"
            android:textStyle="normal"
            android:text="常见问题"
            android:textSize="@dimen/px_36"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </RelativeLayout>
    <LinearLayout
        android:background="@color/cl_f5f5f5"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <RelativeLayout
        android:layout_marginTop="@dimen/px_30"
        android:background="@color/white"
        android:paddingLeft="@dimen/px_32"
        android:paddingRight="@dimen/px_32"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_100">
        <TextView
            android:text="蜗牛APP使用方法"
            android:layout_centerVertical="true"
            android:textSize="@dimen/px_32"
            android:textColor="@color/cl_333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_width="@dimen/px_16"
            android:layout_height="@dimen/px_28"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/youjian" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_1"
        android:layout_below="@+id/region"
        android:background="@color/cl_e3e3e3" />
    <RelativeLayout
        android:background="@color/white"
        android:paddingLeft="@dimen/px_32"
        android:paddingRight="@dimen/px_32"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_100">
        <TextView
            android:text="收费制度"
            android:layout_centerVertical="true"
            android:textSize="@dimen/px_32"
            android:textColor="@color/cl_333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_width="@dimen/px_16"
            android:layout_height="@dimen/px_28"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/youjian" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_1"
        android:layout_below="@+id/region"
        android:background="@color/cl_e3e3e3" />
    <RelativeLayout
        android:background="@color/white"
        android:paddingLeft="@dimen/px_32"
        android:paddingRight="@dimen/px_32"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_100">
        <TextView
            android:text="超期管理"
            android:layout_centerVertical="true"
            android:textSize="@dimen/px_32"
            android:textColor="@color/cl_333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_width="@dimen/px_16"
            android:layout_height="@dimen/px_28"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/youjian" />
    </RelativeLayout>
    <RelativeLayout
        android:background="@color/white"
        android:paddingLeft="@dimen/px_32"
        android:paddingRight="@dimen/px_32"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_100">
        <TextView
            android:text="蜗牛站分布"
            android:layout_centerVertical="true"
            android:textSize="@dimen/px_32"
            android:textColor="@color/cl_333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_width="@dimen/px_16"
            android:layout_height="@dimen/px_28"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/youjian" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_1"
        android:layout_below="@+id/region"
        android:background="@color/cl_e3e3e3" />
    <RelativeLayout
        android:background="@color/white"
        android:paddingLeft="@dimen/px_32"
        android:paddingRight="@dimen/px_32"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_100">
        <TextView
            android:text="怎么在蜗牛站寄件"
            android:layout_centerVertical="true"
            android:textSize="@dimen/px_32"
            android:textColor="@color/cl_333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_width="@dimen/px_16"
            android:layout_height="@dimen/px_28"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/youjian" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_1"
        android:layout_below="@+id/region"
        android:background="@color/cl_e3e3e3" />
    <RelativeLayout
        android:background="@color/white"
        android:paddingLeft="@dimen/px_32"
        android:paddingRight="@dimen/px_32"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_100">
        <TextView
            android:text="关于打赏"
            android:layout_centerVertical="true"
            android:textSize="@dimen/px_32"
            android:textColor="@color/cl_333333"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <ImageView
            android:layout_width="@dimen/px_16"
            android:layout_height="@dimen/px_28"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:src="@mipmap/youjian" />
    </RelativeLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/px_1"
        android:layout_below="@+id/region"
        android:background="@color/cl_e3e3e3" />
    </LinearLayout>
</LinearLayout>
